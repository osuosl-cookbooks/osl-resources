net:
  port: <%= @port %>
  bindIp: <%= @bind_ip %>
  maxIncomingConnections: <%= @max_connections %>
processManagement:
  fork: <%= @forking %>
  pidFilePath: <%= @pid_file_path %>
  #timeZoneInfo: /usr/share/zoneinfo
storage:
  dbPath: <%= @data_dir %>
  journal:
    enabled: true
systemLog:
  destination: <%= @log_dest %>
  logAppend: true<% if @log_dest == 'file' -%>
  path: <%= @log_path %><% end -%>
